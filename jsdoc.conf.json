{
	"source": {
		"includePattern": "src/.+\\.js?$",
        "include": "src"
	},
    "tags": {
    },
    "plugins": [
        "./jsdoc/tags/index.js"
    ],
    "opts": {
        "destination": "./documentation",
        "recurse": true,
        "encoding": "utf8",
        "template": "node_modules/jsdoc-rtd"
    }
}
